package day0105;

public class IfOper_11 {

	public static void main(String[] args) {
		//if문과 삼항연산자 비교
		
		int score = 80;
		String grade; 
		
		
		if(score >= 90) 
			grade= "A";
		else if(score>= 80)
			grade = "B";
		else if(score>= 70)
			grade = "C";
		else if(score>= 60)
			grade = "D";
		else 
			grade = "F";
		System.out.println("학점은" + grade + "입니다.");
	}

}

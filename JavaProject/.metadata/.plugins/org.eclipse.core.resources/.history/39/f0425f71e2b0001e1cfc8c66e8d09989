package day0112;

class Food{
	
	private	String foodName;
	private int fPrice;
	
	static int no;
	
	//명시적생성자(2개)
	public Food(String name, int price) {
		this.foodName = name;
		this.fPrice = price;
	}
	//출력문
	public void run() {
		System.out.println("[메뉴_"+no"]");
		
	}
	
	
	/*
	 * [메뉴_1]
	 * 음식명: 마르게리타 피자
	 * 가격: 12000원
	 * --------------------
	 * [메뉴_2]
	 * 음식명: 까르보나라 스파게티
	 * 가격:15000원
	 * ---------------------
	 * [메뉴_3]
	 * 음식명:리코타 샐러드
	 * 가격: 9000원
	 * 
	 * 
	 */
}


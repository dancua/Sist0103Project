package day0110;

class Ipgo {
	private String sangName;
	private int Price;
	
	public void setSangName(String sangName) {
		this.sangName = sangName;
	}
	public void setPrice(int price) {
		Price = price;
	}
	public String getSangName() {
		return sangName;
	}
	public int getPrice() {
		return Price;
	}
	
}
// 사과, 2500 오렌지,4400
public class SangpumIpgo_13 {

	public static void main(String[] args) {
		
		Ipgo ig = new Ipgo();
		
		ig.setSangName("사과");
		ig.setPrice(2500);
		
		ig.setSangName("오렌지");
		ig.setPrice(4400);
		
		ig.getSangName();

	}

}

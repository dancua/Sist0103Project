package day0112;

class Emart{
	
	private String sangpum;
	private int su;
	
	public Emart() {
			sangpum = "딸기";
			su = 10;
			// this("딸기",10); 
}
	public Emart(String sangpum,int su) {
		this.sangpum = sangpum;
		this.su = su;
		
	}
	public void writeMart()
	{
		System.out.println("상품명: "+ sangpum);
	}
	
	
}

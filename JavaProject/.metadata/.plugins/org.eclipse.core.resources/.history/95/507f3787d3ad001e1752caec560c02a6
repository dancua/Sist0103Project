package day0108;

public class For_09 {

	public static void main(String[] args) {
		// 가로로 1~10까지 출력 
		
		System.out.println("**가로 1~10 출력 결과**");
		for(int i = 0; i <= 10; i++)
		{
			System.out.print(i);
		}
		System.out.println("종료");

		//10~1까지 출력
		int e = 10;
		while(true)
		{
			System.out.print(e--);
			if(e < 0 )
				break;
		}
		
	}

}

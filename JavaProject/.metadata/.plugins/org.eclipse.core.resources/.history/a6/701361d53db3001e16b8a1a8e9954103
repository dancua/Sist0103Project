package day0115;

interface InterAA{
	
	public void play();
}
//class ClassA{	
//////////////////////
interface InterBB {
	
	public void draw();
}
///////////////////////
class InterImple_01 implements InterAA,InterBB{

	@Override
	public void draw() {
		System.out.println("나는 그림을 그린다.");
		
	}

	@Override
	public void play() {
		System.out.println("나는 게임을 한다.");
		
	}
	
	
	
}
public class InheritReview_01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		InterImple 
		
	}
	

}

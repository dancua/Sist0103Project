package day0112;
/*
 * [쌍용매장 입고상품]
 * =================
 * 번호		상품명	가격		색상
 * 1		요가매트	25000원	핑크	
 * 2		아령		30000원	블랙
 * 3 		레깅스	45000원	베이지
 * 4		나시		22000원	화이트
 */

class Shop{
	String sangpum;
	String sColor;
	int sPrice;
	static int cnt;
	
	public void sShop(String name, String color, int price) {
		this.sangpum = name;
		this.sColor = color;
		this.sPrice = price;
	}
	void run() {
		System.out.println("[쌍용매장 입고상품]");
		System.out.println("번호\t" + "상품명\t" + "가격\t" + "색상\t");
		System.out.println("===============================");
	}
	
}



